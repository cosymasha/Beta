{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"clientId": {
			"type": "string"
		},
		"guildId": {
			"type": "string"
		},
    "guildCommand": {
			"type": "boolean",
      "description": "true: ギルドコマンド false: グローバルコマンド"
		},
		"blackList": {
			"type": "object",
			"properties": {
				"guilds": {
					"type": "array",
					"items": {}
				},
				"users": {
					"type": "array",
					"items": {}
				}
			},
			"required": [
				"guilds",
				"users"
			]
		},
		"beta": {
			"type": "object",
			"properties": {
				"betaMode": {
					"type": "boolean",
          "description": "この機能について理解してないないならfalseをおすすめする"
				},
				"guildId": {
					"type": "string"
				},
				"roleId": {
					"type": "string"
				}
			},
			"required": [
				"betaMode",
				"guildId",
				"roleId"
			]
		}
	},
	"required": [
		"$schema",
		"clientId",
		"guildCommand",
		"guildId",
		"blackList",
		"beta"
	]
}