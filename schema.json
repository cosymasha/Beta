{
	"type": "object",
	"properties": {
		"clientId": {
			"type": "string",
			"description": "BOTのクライアントID",
			"maxLength": 19
		},
		"guildId": {
			"type": "string",
			"description": "GuildCommandとして登録するサーバーID",
			"maxLength": 19
		},
		"statusMessage": {
			"type": "string",
			"description": "BOTのステータスメッセージ"
		},
		"dbName": {
			"type": "string",
			"description": "MongoDBのデータベース名"
		},
		"blackList": {
			"type": "object",
			"description": "このリストに該当するサーバーはBOTの全機能が使用できなくなります",
			"properties": {
				"guilds": {
					"type": "array",
					"description": "サーバーID",
					"maxLength": 19,
					"items": {}
				},
				"users": {
					"type": "array",
					"description": "ユーザーID (このユーザーがオーナーのサーバーが全てブロックされます)",
					"maxLength": 19,
					"items": {}
				}
			},
			"required": [ "guilds", "users" ]
		},
		"beta": {
			"type": "object",
			"description": "サーバーのオーナーに指定したサーバーのロールが付与されていないとBOTの全機能が使用できなくなります (注意: これを有効にするとBOTの動作が若干遅くなります)",
			"properties": {
				"enable": {
					"type": "boolean",
					"description": "BETAモードを有効にするか？"
				},
				"guildId": {
					"type": "string",
					"description": "サーバーID",
					"maxLength": 19
				},
				"roleId": {
					"type": "string",
					"description": "ロールID",
					"maxLength": 19
				}
			},
			"required": [ "enable", "guildId", "roleId" ]
		}
	},
	"required": [ "$schema", "clientId", "guildCommand", "statusMessage", "dbName", "blackList", "beta" ]
}