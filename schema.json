{
	"type": "object",
	"properties": {
		"clientId": {
			"type": "string",
			"description": "BOTのクライアントID",
			"maxLength": 19
		},
		"guildId": {
			"type": "string",
			"description": "GuildCommandとして登録するサーバーID",
			"maxLength": 19
		},
		"guildCommandMode": {
			"type": "boolean",
			"description": "一般向けコマンドをGuildCommandとして登録するか"
		},
		"statusMessage": {
			"type": "string",
			"description": "BOTのステータスメッセージ"
		},
		"dbName": {
			"type": "string",
			"description": "MongoDBのデータベース名"
		},
		"Admin": {
			"type": "object",
			"description": "取り扱い注意！Discord内でBOTの管理が行えるようになります",
			"properties": {
				"guild": {
					"type": "string",
					"description": "管理者用コマンドを登録するサーバーID",
					"maxLength": 19
				},"users": {
					"type": "array",
					"description": "管理者用コマンドの使用を許可するユーザーID",
					"maxLength": 19,
					"items": {}
				}
			}
		},
		"blackList": {
			"type": "object",
			"description": "このリストに該当するサーバーはBOTの全機能が使用できなくなります",
			"properties": {
				"guilds": {
					"type": "array",
					"description": "サーバーID",
					"maxLength": 19,
					"items": {}
				},
				"users": {
					"type": "array",
					"description": "ユーザーID (このユーザーがオーナーのサーバーが全てブロックされます)",
					"maxLength": 19,
					"items": {}
				}
			},
			"required": [ "guilds", "users" ]
		}
	},
	"required": [ "$schema", "clientId", "guildCommand", "statusMessage", "dbName", "blackList", "beta", "ownerGuildId", "guildCommandMode" ]
}